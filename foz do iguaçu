    <script>
      if (
        document.location.href.indexOf("/assine/") === -1 &&
        document.location.href.indexOf("/assinar/") === -1 &&
        document.location.href.indexOf("/login") === -1 &&
        document.location.href.indexOf("/ofertas") === -1 &&
        document.location.href.indexOf("/login-vivo-fibra") === -1 &&
        document.location.href.indexOf("/btg-especial-financas") === -1 &&
        document.location.href.indexOf("/25-viagens-para-fazer-em-2025-especial-btg") === -1 &&
        document.location.href.indexOf("assine.abril.com.br") === -1
      ) {

        function addScriptToHead() {
          var scriptElement = document.createElement('script');
          scriptElement.src = 'https://viagemeturismo.abril.com.br/wp-content/themes/abril-master2/js/abrad.min.js?ver=2.2.4';
          document.body.appendChild(scriptElement);
        }

        function handleScroll() {
          if (window.scriptAdded === true) return true;
          window.scriptAdded = true;

          eventos.forEach(function (evento) {
            window.removeEventListener(evento, handleScroll);
          });

          addScriptToHead();
        }

        if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
            var eventos = ["scroll"];
        } else {
            var eventos = ["keydown", "mousemove", "touchmove", "touchstart", "touchend", "wheel", "scroll", "click"];
        }

        eventos.forEach(function(evento) {
            window.addEventListener(evento, handleScroll);
        });
      }
    </script>
  
